
  <div><%= link_to 'ログアウト', logout_path, method: :delete %></div>
  
  <div><%= 'ログインユーザ名: ' + @current_user.name %></div>
  
  <h1>タスク一覧</h1>
  
  <ul>
  
  <% @tasks.each do |task| %>
  
  <li>
    <%= link_to task.id, task %>
    <%= link_to task.user.name, user_path(task.user) %>
    <%= task.content %>
    <%= task.status %>
  </li>
  
  <% end %>
    
  </ul>
  
  <%= link_to '新規作成タスクの投稿', new_task_path %>
  <%= link_to 'ユーザ一覧', users_path %>


